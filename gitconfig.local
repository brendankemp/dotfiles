[alias]
  br = branch
  fetch-rebase-qa = !sh -c 'git fetch && git rebase origin/qa' -
  lg = log --all --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  s = status
[core]
  excludesfile = ~/.gitignore_global
[credentials]
  helper = osxkeychain
[fetch]
  prune = true
[pull]
  rebase = true
[status]
  branch = true
  short = true
[user]
  email = brendan.kemp@gmail.com
  name = Brendan Kemp
