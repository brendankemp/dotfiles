[alias]
  fetch-rebase-qa = !sh -c 'git fetch && git rebase origin/qa' -
  fetch-rebase-qa-push = !sh -c 'git fetch-rebase-qa && git push -u' -
  l = log --pretty='%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset'
[core]
  excludesfile = ~/.gitignore_global
[credentials]
  helper = osxkeychain
